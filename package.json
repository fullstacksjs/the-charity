{
  "name": "the-charity-frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:local": "dotenv -e .env.dev -- npm run dev:ci",
    "dev:ci": "vite",
    "dev": "npm-run-all -l -p codegen:watch dev:local",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "./scripts/prepare",
    "lint:ci": "eslint . --ext .ts,.tsx",
    "lint": "npm run lint:ci -- --fix",
    "spell": "./scripts/spell",
    "test": "vitest",
    "test:ci": "npm run test",
    "coverage": "vitest run --coverage",
    "storybook": "dotenv -e .env.dev -- start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pr": "./scripts/pr",
    "cy:component:run": "dotenv -e .env.dev -- cypress run --component",
    "cy:e2e:run": "dotenv -e .env.dev -- cypress run",
    "cy:e2e:open": "dotenv -e .env.dev -- cypress open",
    "codegen": "dotenv -e .env.dev -- graphql-codegen",
    "codegen:watch": "npm run codegen -- --watch"
  },
  "dependencies": {
    "@apollo/client": "3.7.0",
    "@emotion/react": "11.10.4",
    "@fullstacksjs/toolbox": "2.21.0",
    "@hookform/resolvers": "2.9.9",
    "@mantine/core": "5.5.5",
    "@mantine/hooks": "5.5.5",
    "@tanstack/react-location": "3.7.4",
    "dotenv-cli": "6.0.0",
    "graphql": "16.6.0",
    "jsdom": "20.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.37.0",
    "runtypes": "6.6.0",
    "stylis": "4.1.2",
    "stylis-plugin-rtl": "2.1.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@fullstacksjs/eslint-config": "8.9.0",
    "@fullstacksjs/tsconfig": "0.6.0",
    "@graphql-codegen/add": "3.2.1",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/typescript": "2.7.4",
    "@graphql-codegen/typescript-operations": "2.5.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.4",
    "@storybook/addon-a11y": "6.5.12",
    "@storybook/addon-actions": "6.5.12",
    "@storybook/addon-essentials": "6.5.12",
    "@storybook/addon-interactions": "6.5.12",
    "@storybook/addon-links": "6.5.12",
    "@storybook/builder-vite": "0.2.4",
    "@storybook/react": "6.5.12",
    "@storybook/testing-library": "0.0.13",
    "@storybook/testing-react": "1.3.0",
    "@testing-library/cypress": "8.0.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@vitejs/plugin-react": "2.1.0",
    "babel-loader": "8.2.5",
    "chromatic": "6.10.2",
    "cspell": "6.12.0",
    "cypress": "10.10.0",
    "dotenv": "16.0.3",
    "dotenv-cli": "6.0.0",
    "eslint": "8.25.0",
    "graphql-config": "4.3.6",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "start-server-and-test": "1.14.0",
    "storybook-addon-apollo-client": "4.0.13",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vite-plugin-svgr": "2.2.2",
    "vite-tsconfig-paths": "3.5.1",
    "vitest": "0.24.3"
  },
  "volta": {
    "node": "16.15.1",
    "npm": "8.18.0"
  }
}
